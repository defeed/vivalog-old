.well.well-default
  .row
    .col-sm-4
      %dl.dl-horizontal
        %dt= t('projects.attributes.volume')
        %dd= volume(@project.volume)
    .col-sm-4
      %dl.dl-horizontal
        %dt= t('projects.num_workers')
        %dd= @project.workers.count
  .row
    .col-sm-4
      %dl.dl-horizontal
        %dt= t('projects.attributes.sum_receive')
        %dd= eur(@project.sum_receive)
    .col-sm-4
      %dl.dl-horizontal
        %dt= t('projects.attributes.sum_polish')
        %dd= eur(@project.sum_polish)
    .col-sm-4
      %dl.dl-horizontal
        %dt= t('projects.attributes.hourly_rate')
        %dd= eur(@project.hourly_rate)
